{
	"name": "universe-lm",
	"version": "0.0.1",
	"private": true,
	"author": {
		"name": "Ameer Arsala",
		"url": "https://github.com/AmeerArsala/UniverseLM"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/AmeerArsala/UniverseLM.git",
		"directory": "UniverseLM"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier \".\" --check && eslint --report-unused-disable-directives --max-warnings 0",
		"format": "prettier \".\" --write --ignore-path .gitignore",
		"taze": "npx taze major -I",
		"taze:minor": "npx taze minor -w",
		"start": "pnpm run build && pnpm run preview",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/typography": "^0.5.13",
		"@types/eslint": "^8.56.10",
		"autoprefixer": "^10.4.19",
		"eslint": "^9.2.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.38.0",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"axios": "^1.6.8",
		"clsx": "^2.1.1",
		"mdsvex": "^0.11.0",
		"svelte-motion": "^0.12.2",
		"tailwind-merge": "^2.3.0"
	}
}